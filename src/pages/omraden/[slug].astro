const { area } = Astro.props as Props;

const schema: SchemaData = {
  "@context": "https://schema.org",
  "@type": "LocalBusiness",
  "name": "Södertälje El",
  "description": `Elektriker i ${area.name}. Vi erbjuder elinstallation, elbilsladdare, renovering och akut hjälp 24/7.`,
  "address": {
    "@type": "PostalAddress",
    "addressLocality": area.name,
    "addressRegion": "Stockholm",
    "postalCode": area.postalCodes[0],
    "addressCountry": "SE"
  },
  "geo": {
    "@type": "GeoCoordinates",
    "latitude": "59.1954",
    "longitude": "17.6255"
  },
  "areaServed": {
    "@type": "City",
    "name": area.name
  },
  "hasOfferCatalog": {
    "@type": "OfferCatalog",
    "name": "Elektriker-tjänster",
    "itemListElement": area.services.map((service: Service): SchemaOffer => ({
      "@type": "Offer",
      "itemOffered": {
        "@type": "Service",
        "name": service.name,
        "description": service.description
      },
      "priceSpecification": {
        "@type": "PriceSpecification",
        "price": service.price
      }
    }))
  }
};
---

<Layout
  title={area.title}
  description={area.description}
>
  <Header slot="header" />

  <Hero
    badge={`Elektriker i ${area.name}`}
    title={`Behöver du elektriker i ${area.name}?`}
    description={`Din lokala elektriker i ${area.name}. Vi erbjuder snabb service, fasta priser och 100% nöjd-kund-garanti. Akut hjälp inom 60 minuter, dygnet runt.`}
    image={area.image}
    showCTA={true}
    showHighlights={true}
  />

  <!-- Main Content -->
  <section class="py-16">
    <div class="container mx-auto px-4">
      <div class="grid md:grid-cols-12 gap-12">
        <!-- Content -->
        <div class="md:col-span-8">
          <div class="prose prose-lg max-w-none">
            {area.content.split('\n\n').map((paragraph: string) => (
              <div class="content-block">
                {paragraph.includes('•') ? (
                  <ul class="list-none space-y-2">
                    {paragraph.split('\n').map((line: string) => (
                      <li class={line.startsWith('•') ? 'flex items-start' : 'font-semibold text-xl mb-4'}>
                        {line.startsWith('•') ? (
                          <>
                            <span class="text-primary-600 mr-2">{line.substring(0, 1)}</span>
                            <span>{line.substring(2)}</span>
                          </>
                        ) : (
                          line
                        )}
                      </li>
                    ))}
                  </ul>
                ) : (
                  paragraph.split('\n').map((line: string) => (
                    line.length > 0 && (
                      line === paragraph.split('\n')[0] && !line.includes(':') ? (
                        <h2 class="text-3xl font-bold mb-6">{line}</h2>
                      ) : line.endsWith(':') ? (
                        <h3 class="text-xl font-semibold mb-4">{line}</h3>
                      ) : (
                        <p class="mb-6">{line}</p>
                      )
                    )
                  ))
                )}
              </div>
            ))}
          </div>
        </div>

        <!-- Sidebar -->
        <div class="md:col-span-4">
          <div class="sticky top-24 space-y-8">
            <!-- Services -->
            <div class="bg-white rounded-lg shadow-sm border border-gray-100 overflow-hidden">
              <div class="p-6">
                <h3 class="text-xl font-bold mb-4">Våra tjänster</h3>
                <div class="space-y-4">
                  {area.services.map((service: Service) => (
                    <div class="border-b border-gray-100 last:border-0 pb-4 last:pb-0">
                      <h4 class="font-semibold mb-1">{service.name}</h4>
                      <p class="text-sm text-gray-600 mb-2">{service.description}</p>
                      <p class="text-sm font-medium text-primary-600">{service.price}</p>
                    </div>
                  ))}
                </div>
              </div>
            </div>

            <!-- Contact Form -->
            <div class="bg-gray-50 rounded-lg p-6">
              <h3 class="text-xl font-bold mb-4">Kontakta oss</h3>
              <p class="text-gray-600 mb-6">
                Behöver du hjälp av en elektriker i {area.name}? 
                Vi återkommer med offert inom 2 timmar på vardagar.
              </p>
              <ContactForm
                title="Få kostnadsfri offert"
                description={`Vi hjälper dig i ${area.name}`}
                submitLabel="Skicka förfrågan"
              />
            </div>

            <!-- Coverage Area -->
            <div class="bg-white rounded-lg shadow-sm border border-gray-100 p-6">
              <h3 class="text-xl font-bold mb-4">Täckningsområde</h3>
              <div class="space-y-2">
                <p class="text-gray-600 mb-4">Vi täcker följande postnummer:</p>
                <div class="flex flex-wrap gap-2">
                  {area.postalCodes.map((code: string) => (
                    <span class="px-3 py-1 bg-gray-100 rounded-full text-sm text-gray-600">
                      {code}
                    </span>
                  ))}
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- CTA Section -->
  <section class="bg-primary-900 text-white py-16">
    <div class="container mx-auto px-4">
      <div class="max-w-3xl mx-auto text-center">
        <h2 class="text-3xl font-bold mb-6">Behöver du elektriker i {area.name}?</h2>
        <p class="text-xl text-gray-100 mb-8">
          Vi finns nära dig och kan snabbt vara på plats. Kontakta oss för en kostnadsfri offert!
        </p>
        <div class="flex flex-col sm:flex-row justify-center gap-4">
          <a
            href="tel:08-123 456 78"
            class="inline-flex items-center justify-center px-6 py-3 border border-transparent text-base font-medium rounded-md text-primary-700 bg-white hover:bg-gray-50"
          >
            Ring akut: 08-123 456 78
          </a>
          <a
            href="/kontakt#contact"
            class="inline-flex items-center justify-center px-6 py-3 border border-white text-base font-medium rounded-md text-white hover:bg-primary-800"
          >
            Få kostnadsfri offert
          </a>
        </div>
      </div>
    </div>
  </section>

  <Footer slot="footer" />

  <!-- Schema.org JSON-LD -->
  <script type="application/ld+json" set:html={JSON.stringify(schema)} />
</Layout> 